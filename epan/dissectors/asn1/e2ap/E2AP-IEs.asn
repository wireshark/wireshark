ENB-ID-Choice ::= CHOICE {
        enb-ID-macro			BIT STRING (SIZE(20)),
        enb-ID-shortmacro		BIT STRING (SIZE(18)),
        enb-ID-longmacro		BIT STRING (SIZE(21)),
        ...
}

