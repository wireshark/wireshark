ENB-ID ::= CHOICE {
        macro-eNB-ID			BIT STRING (SIZE (20)),
        home-eNB-ID				BIT STRING (SIZE (28)),
        ... ,
        short-Macro-eNB-ID		BIT STRING (SIZE(18)),
        long-Macro-eNB-ID		BIT STRING (SIZE(21))
}
