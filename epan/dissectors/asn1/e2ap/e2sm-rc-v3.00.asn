        ...
}

E2SM-RC-EventTrigger-Format3-Item ::= SEQUENCE {
        ric-eventTriggerCondition-ID		RIC-EventTriggerCondition-ID,
        e2NodeInfoChange-ID					INTEGER (1..512, ...),
