#!/bin/sh

set -e

GROUP=wireshark

. /usr/share/debconf/confmodule

if [ "$1" = "purge" ] ; then
    if getent group $GROUP >/dev/null; then
        if [ -x "$(command -v delgroup)" ]; then
            if ! delgroup --quiet --system $GROUP; then
                db_input high wireshark-common/group-removal-failed || true
                db_go
            fi
        fi
    fi
fi

#DEBHELPER#
