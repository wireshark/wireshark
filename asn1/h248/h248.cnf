    h248_cmdmsg->msg_type = H248_TRX_REPLY;
    h248_cmdmsg->term_is_wildcard = FALSE;
#.END

#.FN_FTR CommandReply
    if (check_col(pinfo->cinfo, COL_INFO)) col_set_str(pinfo->cinfo, COL_INFO, cmd_str(h248_cmdmsg));    
