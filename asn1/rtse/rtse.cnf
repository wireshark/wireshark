#.FN_BODY RTTPapdu  VAL_PTR=&priority
  int priority = -1;

  col_append_str(actx->pinfo->cinfo, COL_INFO, "Turn-Please");

  %(DEFAULT_BODY)s

  if(priority != -1)
    col_append_fstr(actx->pinfo->cinfo, COL_INFO, " (%%d)", priority);

